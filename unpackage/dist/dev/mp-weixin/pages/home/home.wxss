.container .my-search {
  display: flex;
  align-items: center;
  justify-content: center;
  margin: 0 auto;
  padding: 0 20px;
  width: 80%;
  height: 35px;
  border-radius: 10px;
  background-color: #e8e8e8;
}
.container .my-search .my-search-box1 {
  display: flex;
  justify-content: center;
  align-items: center;
}
.container .my-search .my-search-box1 text {
  font-size: 15px;
  color: #808080;
}
.container .contant {
  display: flex;
  align-items: center;
  justify-content: center;
  margin: 0 auto;
  width: 90%;
}
.container .contant .contant_box .movie_box .titel {
  margin-top: 10px;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.container .contant .contant_box .movie_box .titel .tiel {
  font-style: italic;
  font-size: 20px;
  color: #ffaaff;
}
.container .contant .contant_box .movie_box .titel .na1 {
  font-size: 15px;
  color: #48d900;
}
.container .contant .contant_box .movie_box2 .contant_box2 {
  white-space: nowrap;
}
.container .contant .contant_box .movie_box2 .contant_box2 .movie {
  padding: 0;
  margin-top: 10px;
  display: inline-block;
  width: 90px;
  height: 150px;
}
.container .contant .contant_box .movie_box2 .contant_box2 .movie image {
  border-radius: 7px;
  width: 80%;
  height: 80%;
}
.container .contant .contant_box .movie_box2 .contant_box2 .movie .text {
  width: 80%;
  font-style: italic;
  font-size: 10px;
}
.container .contant .contant_box .movie_box2 .contant_box2 .movie .rate {
  width: 80%;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.container .contant .contant_box .movie_box2 .contant_box2 .movie .rate .rate1 {
  font-size: 10px;
  text-align: right;
}
.container .footer {
  height: 50px;
  text-align: center;
  line-height: 60px;
  font-size: 14px;
  color: #cecece;
}

