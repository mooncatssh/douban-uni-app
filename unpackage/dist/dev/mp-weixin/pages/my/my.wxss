.container .login {
  display: flex;
  align-items: center;
  width: 100%;
  height: 150px;
  background-color: #ffaaff;
}
.container .login .btn {
  width: 50%;
  height: 40px;
  margin: 0 auto;
  background-color: white;
  border-radius: 20px;
  text-align: center;
  line-height: 40px;
  font-size: 20px;
  color: #aa00ff;
}
.container .main {
  width: 90%;
  margin: 0 auto;
}
.container .main .title {
  display: flex;
  justify-content: space-between;
  margin-top: 10px;
}
.container .main .title .left {
  font-size: 20px;
  color: #ffaa7f;
}
.container .main .title .right {
  font-size: 13px;
  color: #c1c1c1;
}
.container .center {
  width: 100%;
  margin: 0 auto;
}
.container .center .center_box1 {
  height: 62px;
  display: flex;
  justify-content: space-between;
  align-items: center;
  border-bottom: 0.3px solid #ffaaff;
}
.container .center .center_box1 .left {
  margin-top: 20px;
  width: 120px;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.container .center .center_box1 .left image {
  width: 50px;
  height: 30px;
}
.container .center .center_box1 .left .left_left {
  margin-left: 20px;
  width: 150px;
}
.container .center .center_box1 .left .left_left .text1 {
  font-size: 14px;
  color: #aa00ff;
}
.container .center .center_box1 .left .left_left .text2 {
  margin-top: 10px;
  font-size: 15px;
  color: #c1c1c1;
}
.container .center .center_box1 .right {
  width: 150px;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.container .center .center_box1 .right .right_right {
  margin-top: 40px;
  width: 70px;
  font-size: 10px;
  color: #c1c1c1;
}
.container .center .center_box1 .right .right_btn {
  margin-top: 40px;
  border-radius: 5px;
  width: 80px;
  background-color: #ffaaff;
  height: 20px;
  font-size: 13px;
  color: #aa00ff;
  text-align: center;
  line-height: 20px;
}
.container .center .center_box1 .right .icon {
  margin-top: 40px;
}
.container .footer {
  width: 100%;
  margin: 0 auto;
  margin-top: 3px;
}
.container .footer .footer_box1 {
  margin-top: 5px;
  font-size: 13px;
  color: #b6b6b6;
}
.container .footer .footer_box2 {
  margin-top: 5px;
  font-size: 15px;
  color: #b6b6b6;
}
.container .footer .footer_box3 {
  margin-top: 5px;
  font-size: 15px;
  color: #b6b6b6;
}

