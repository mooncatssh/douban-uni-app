.container {
  margin-top: 130px;
}
.container .login {
  width: 70%;
  height: 50px;
  border-radius: 10px;
  margin: auto;
  text-align: center;
  line-height: 50px;
  background-color: #ffaaff;
  color: #aa55ff;
}
.container .login_rad {
  width: 70%;
  margin-left: 30px;
  position: absolute;
  bottom: 5px;
  -webkit-transform: scale(0.7, 0.7);
          transform: scale(0.7, 0.7);
}
.container .footer {
  width: 70%;
  margin-left: 80px;
  position: absolute;
  bottom: 10px;
  font-size: 13px;
}
.container .footer .text {
  color: #ffaaff;
}
.container .login_titel {
  width: 80%;
  text-align: center;
  margin: 0 auto;
  font-size: 30px;
  color: #ffaaff;
}
.container .login_tite2 {
  width: 75%;
  text-align: center;
  margin: 0 auto;
  font-size: 15px;
  color: #d18bd1;
}
.container .uni-input1 {
  height: 40px;
  width: 80%;
  margin: 0 auto;
  margin-top: 10px;
  color: #aa55ff;
  font-size: 15px;
  border-radius: 5px;
  background-color: #eaeaea;
}
.container .login_btn {
  width: 40%;
  margin: 0 auto;
  margin-top: 10px;
  text-align: center;
  height: 30px;
  line-height: 30px;
  border-radius: 5px;
  background-color: #ffaaff;
  color: #aa55ff;
}

